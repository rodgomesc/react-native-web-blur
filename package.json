    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native-web": "^0.17.0",
    "typescript": "^4.3.5",
