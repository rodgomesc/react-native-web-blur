  "description": "React Native for Web implementation of @react-native-community/blur",
  "main": "dist/index.js",
  "main-es": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-native-web-community/react-native-web-blur.git"
  },
  "author": {
    "name": "Rodrigo Gomes da Cunha",
    "email": "rodgomesc@gmail.com",
